    "from xgboost import XGBClassifier\n",
    "import pandas as pd\n",
    "\n",
    "data = pd.read_csv(\"heart.csv\")\n",
    "X = data.drop(\"target\",axis=1)\n",
    "y = data[\"target\"]\n",
    "\n",
    "model = XGBClassifier()\n",
    "model.fit(X,y)\n",
    "\n",
    "print(\"Heart Model Ready\")"
   ]
